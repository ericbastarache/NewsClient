{
  "name": "news-client",
  "productName": "Angular News Client",
  "description": "Application for viewing news from various sources",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng build --base-href ./dist && electron .",
    "test": "ng test",
    "package-mac": "ng build --environment=prod --base-href ./dist && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icons/icon.icns --prune = true --out=release-builds",
    "package-win": "ng build --environment=prod --base-href ./dist && electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Angular News Client\"",
    "create-mac-installer": "",
    "create-win-installer": "",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.30",
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "electron": "^1.4.15",
    "electron-installer-dmg": "^0.2.0",
    "electron-packager": "^8.5.1",
    "electron-reload": "^1.1.0",
    "electron-winstaller": "^2.5.2",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
}
